<h1>Listing coupons</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Url</th>
    <th>Description</th>
    <th>Affurl</th>
    <th>Imageurl</th>
    <th>Category</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @coupons.each do |coupon| %>
  <tr>
    <td><%= coupon.title %></td>
    <td><%= coupon.url %></td>
    <td><%= coupon.description %></td>
    <td><%= coupon.affurl %></td>
    <td><%= coupon.imageurl %></td>
    <td><%= coupon.category %></td>
    <td><%= link_to 'Show', coupon %></td>
    <td><%= link_to 'Edit', edit_coupon_path(coupon) %></td>
    <td><%= link_to 'Destroy', coupon, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Coupon', new_coupon_path %>
